{
  "name": "event",
  "version": "1.0.0",
  "description": "Event Manager Server and client",
  "main": "app.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "start": "npm run build:client && npm run start:server",
    "test": "nyc --reporter=lcov --reporter=html --reporter=text mocha --exit ./server/tests/api-test.js --compilers js:babel-register || ECHO.",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "start:dev": "npm run test && coverage",
    "start:server": "babel-watch server/app.js",
    "build:client": "webpack --config client/webpack.config.js",
    "client:dev": "webpack --watch --config client/webpack.config.js",
    "dev": "concurrently \"npm run client:dev\" \"npm run start:server\""
  },
  "author": "Jessica",
  "license": "ISC",
