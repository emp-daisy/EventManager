{
  "name": "event",
  "version": "1.0.0",
  "description": "Event Manager Server and client",
  "main": "app.js",
  "author": "Jessica",
  "license": "ISC",
  "scripts": {
    "test:db": "psql -U postgres -c 'drop database db_events_test;' && psql -c 'create database db_events_test;' -U postgres",
    "migrate": "sequelize db:migrate",
    "undo:migrate": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "start": "npm run build:client && npm run start:server",
    "test": "npm run test:server",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "start:dev": "npm run test && coverage",
    "test:client": "jest",
    "test:client:watch": "jest --watch",
    "test:local:server": "NODE_ENV=test npm run test:db && NODE_ENV=test npm run migrate && NODE_ENV=test npm run seed && nyc --reporter=lcov --reporter=html --reporter=text mocha --exit ./server/tests/*.tests.js --compilers js:babel-register || ECHO.",
    "test:server": "nyc --reporter=lcov --reporter=html --reporter=text mocha --exit ./server/tests/*.tests.js --compilers js:babel-register || ECHO.",
    "start:server": "babel-watch server/app.js",
    "build:client": "webpack --config client/webpack.config.js",
    "client:dev": "webpack --watch --config client/webpack.dev.config.js",
    "dev": "concurrently \"npm run client:dev\" \"npm run start:server\""
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./client/src/tests/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(s?css|less|otf|webp|svg|ttf|woff|woff2|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|gif|png|mp4|mkv|avi|webm|swf|wav|mid)$": "jest-static-stubs/$1"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "nyc": {
    "include": "server",
    "exclude": "**/*.tests.js"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "coveralls": "^3.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "identity-obj-proxy": "^3.0.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.8.0",
    "raf": "^3.4.0",
    "redux-mock-store": "^1.5.1"
  },
  "dependencies": {
    "async": "^2.6.0",
    "autoprefixer": "7.1.6",
